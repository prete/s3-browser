{
  "name": "s3-browser",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.21.2",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run set-version",
    "set-version": "S3B_VERSION=`git log -1 --date=short --pretty=format:%cd` perl -i -pe 's/%S3B_VERSION%/$ENV{S3B_VERSION}/' build/index.html",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "shx": "^0.3.2"
  }
}
